        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="Sample Login Example" enabled="true">
          <stringProp name="WebDriverSampler.script">import java.io.File;
import org.apache.commons.io.FileUtils;
import java.util.Random;
import openqa.selenium.OutputType.*;
import org.openqa.selenium.*;
import java.time.Duration;
import org.openqa.selenium.support.ui.*;
import org.openqa.selenium.support.ui.ExpectedConditions;
import org.openqa.selenium.support.ui.WebDriverWait;
import org.openqa.selenium.interactions.Actions;
import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.interactions.Actions;

String baseUrl = &quot;www.example.com&quot;;    // Replace your URL

WDS.sampleResult.sampleStart();

WDS.browser.get(baseUrl + &quot;/&quot;);       // Open Browser

WebDriverWait wait = new WebDriverWait(WDS.browser, Duration.ofSeconds(600)); 

wait.until(ExpectedConditions.presenceOfElementLocated(By.name(&quot;login&quot;)));  // Wait for the Login form to be visible

Thread.sleep(2000);  // Add wait time if you need

WDS.browser.findElement(By.id(&quot;username&quot;)).sendKeys( new String[] { &quot;__Username__&quot; } );    // Provide Username

WDS.browser.findElement(By.id(&quot;password&quot;)).sendKeys( new String[] { &quot;__Password__&quot; } );  // Provide Password

Thread.sleep(2000);

WDS.browser.findElement(By.name(&quot;login&quot;)).click();  //Click Login Button

wait.until(ExpectedConditions.presenceOfElementLocated(By.xpath(&quot;//a[@title=&apos;Hi There...&apos;]&quot;)));  //Wait for particular element to be visible

Thread.sleep(2000);

WebElement Loading = WDS.browser.findElement(By.xpath(&quot;//span[text()=&apos;Loading...&apos;]&quot;));    

wait.until(ExpectedConditions.invisibilityOf(Loading)); // Wait till browser loads completely

WDS.sampleResult.sampleEnd();</stringProp>
          <stringProp name="WebDriverSampler.parameters"></stringProp>
